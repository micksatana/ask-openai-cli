{
  "name": "ask-openai-cli",
  "version": "1.0.0",
  "description": "CLI to ask OpenAI a question",
  "bin": {
    "ask": "dist/index.js"
  },
  "scripts": {
    "build": "shx echo \"export default '$npm_package_version';\" > ./src/version.ts && tsc && shx chmod 755 ./dist/index.js",
    "ask": "ts-node src/index.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micksatana/poc-openai.git"
  },
  "bugs": {
    "url": "https://github.com/micksatana/poc-openai/issues"
  },
  "homepage": "https://github.com/micksatana/poc-openai#readme",
  "dependencies": {
    "commander": "10.0.0",
    "dotenv-flow": "3.2.0",
    "envalid": "7.3.1",
    "openai": "3.1.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/dotenv-flow": "3.2.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "husky": "^8.0.3",
    "jest": "29.4.1",
    "shx": "^0.3.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.4"
  }
}
